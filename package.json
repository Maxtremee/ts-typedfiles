{
  "name": "ts-typedfiles",
  "version": "1.1.0",
  "description": "Generate filepaths from directory structure",
  "keywords": [
    "typescript",
    "typesafety",
    "filepaths"
  ],
  "author": "https://github.com/Maxtremee",
  "homepage": "https://github.com/Maxtremee/ts-typedfiles",
  "bugs": {
    "url": "https://github.com/Maxtremee/ts-typedfiles/issues"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "bin": {
    "ts-typedfiles": "./bin/cli.js"
  },
  "scripts": {
    "build": "tsup",
    "check": "biome check",
    "ci": "npm run check && npm run typecheck && npm run test && npm run build",
    "changeset": "changeset",
    "local-release": "changeset version && changeset publish",
    "prepublishOnly": "npm run ci",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "citty": "0.1.6"
  },
  "devDependencies": {
    "@changesets/cli": "2.27.12",
    "@types/node": "22.13.1",
    "biome": "0.3.3",
    "tsup": "8.3.6",
    "typescript": "^5.0.0",
    "vitest": "3.0.5"
  },
  "packageManager": "pnpm@10.2.1"
}
